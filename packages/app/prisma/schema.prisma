// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Photos {
  id          Int       @id @unique @default(autoincrement())
  path        String
  filename    String
  width       Int
  height      Int
  filetype    String
  created_at  DateTime
  updated_at  DateTime?
  imported_at DateTime
  flag        String
  trashed     String
}
